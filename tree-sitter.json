{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "modelica",
      "camelcase": "Modelica",
      "title": "Modelica",
      "scope": "source.modelica",
      "file-types": [
        "mo"
      ],
      "injection-regex": "^modelica$",
      "class-name": "TreeSitterModelica"
    }
  ],
  "metadata": {
    "version": "0.2.2",
    "license": "OSMC",
    "description": "Modelica grammar for tree-sitter",
    "authors": [
      {
        "name": "OpenModelica"
      }
    ],
    "links": {
      "repository": "https://github.com/OpenModelica/tree-sitter-modelica"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}